<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="DreamPoeBot.DreamPoe.UpdateWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:lex="http://wpflocalizeextension.codeplex.com" xmlns:exile="clr-namespace:DreamPoeBot.DreamPoe" xmlns:extensions="clr-namespace:DreamPoeBot.WPFLocalizeExtension.Extensions" Loaded="method_1">
  <Grid Loaded="method_0">
    <TextBlock HorizontalAlignment="Left, Stretch" Margin="10,20,10,239" TextWrapping="Wrap" VerticalAlignment="Top, Stretch" Width="Auto" Height="Auto" Text="UpdateInfo" />
