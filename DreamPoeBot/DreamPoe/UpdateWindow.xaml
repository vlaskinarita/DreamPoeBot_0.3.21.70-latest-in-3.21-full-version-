    <Button Name="NoButton" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" MinWidth="75" Width="Auto" Content="Cancel" Click="NoButton_Click" />
  </Grid>
</UserControl>